{
  "name": "Open Environment Extension",
  "version": "1.0",
  "description": "Open current url in another environment replacing host url only",
  "manifest_version": 2,
  "permissions": ["tabs", "storage", "declarativeContent"],
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*, http:s//*/*"],
      "js": ["addEnv.js"]
    }
  ],
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  }
}
