{
  "name": "Open Environment Extension",
  "version": "1.0.5",
  "description": "Open current url in another tab replacing host url only but keeping the rest of the path.",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab"],
  "action": {
    "16": "images/logo.png",
    "24": "images/logo.png",
    "32": "images/logo.png",
    "default_popup": "popup.html",
    "default_title": "Open Environment"
  },
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  }
}
