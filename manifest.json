{
  "name": "Open Environment Extension",
  "version": "1.0",
  "description": "Open current url in another environment!",
  "manifest_version": 2,
  "permissions": ["activeTab", "tabs", "storage", "declarativeContent"],
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*, http:s//*/*"],
      "js": ["addEnv.js"]
    }
  ]
}
